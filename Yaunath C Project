#include <stdio.h>

int main() {
    int ch; 
    float c, f;

    do {
        printf("1.C->F\n2.F->C\n3.Exit\n");
        scanf("%d", &ch);

        if(ch==1) {
            scanf("%f", &c);
            printf("%.2f\n", (c*9/5)+32);
        }
        else if(ch==2) {
            scanf("%f", &f);
            printf("%.2f\n", (f-32)*5/9);
        }
        else if(ch!=3) {
            printf("Invalid\n");
        }

    } while(ch!=3);

    return 0;
}
